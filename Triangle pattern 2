import java.util.Scanner;
class Pyramid {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter rows: ");
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            for(int k=1;k<=i;k++){
                System.out.print(k);
            }
            System.out.println();
        }
    }
}
